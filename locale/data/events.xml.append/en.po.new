#: src-en/data/events.xml.append:1180
msgid ""
"data/events.xml.append$//event[@name=\"RAD_FLESH_4\"]/choice[3]/event/choice[@req=\"rad_flesh_rescue\"]/text"
msgstr "Check out the cage."

#: src-en/data/events.xml.append:1183
msgid ""
"data/events.xml.append$//event[@name=\"RAD_FLESH_4\"]/choice[3]/event/choice[@req=\"rad_flesh_rescue\"]/event/text"
msgstr ""
"You open the cage to find a confused looking medical scientist. Looks like "
"he was on the cruiser they took, but can't remember what happened. He agrees"
