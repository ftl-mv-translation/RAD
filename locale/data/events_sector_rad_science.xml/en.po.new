#: src-en/data/events_sector_rad_science.xml:1130
msgid ""
"data/events_sector_rad_science.xml$//event[@name=\"RAD_MAIN_LEAVE\"]/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_sector_rad_science.xml:1134
msgid ""
"data/events_sector_rad_science.xml$//event[@name=\"RAD_MAIN_LEAVE\"]/choice[2]/text"
msgstr "Do nothing."

